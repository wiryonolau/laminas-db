{
    "name": "itseasy/laminas-db",
    "bin": [
        "bin/itseasy-db"
    ],
    "description": "Laminas Database Wrapper",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Wiryono Lau",
            "email": "wiryonolau@gmail.com"
        }
    ],
    "config": {
        "platform": {
            "php": "7.4.30",
            "ext-intl": "1"
        }
    },
    "require": {
        "php": "^7.4",
        "laminas/laminas-db": "^2.13",
        "laminas/laminas-hydrator": "^4.3",
        "laminas/laminas-log": "^2.15",
        "itseasy/util": "^1.0",
        "symfony/console": "^5.4",
        "composer/semver": "^3.3",
        "laminas/laminas-eventmanager": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Itseasy\\Database\\": "src/",
            "Itseasy\\Repository\\": "src/Repository/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Itseasy\\DatabaseTest\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    }
}
